<div>
	<label for="send_message">Send Message:</label>
	<textarea cols="50" rows="5" id="message" name= "message[message]" placeholder="Message"></textarea>
	<textarea cols="50" rows="5" id="public_id" name= "message[passphrase]" placeholder="Passphrase"></textarea>
	<textarea cols="50" rows="5" id="passphrase" name= "message[passphrase]" placeholder="Passphrase"></textarea>
	<textarea cols="50" rows="5" id="salt" name= "message[salt]" placeholder="Salt"></textarea>

	<button class='submit' type="submit" value="Submit">

</div>

<form id="theForm" onsubmit="return false;">
	    <input type='text' name='message' value='message' />
        <input type='text' name='public_id' value='public_id' />
        <input type='text' name='passphrase' value='passphrase' />
        <input type='submit' name='submit' value='submit' />
</form>
